    <table class="table table-hover" id="feed-table">
      <thead>
        <tr>
          <th scope="col">Username</th>
          <th scope="col">Date</th>
          <th scope="col">Exercise Name</th>
          <th scope="col">Sets</th>
          <th scope="col">Reps</th>
          <th scope="col">Weight</th>
          <th scope="col">Effort Taken</th>
          <th scope="col">Rest Time</th>
        </tr>
      </thead>
    {{#each posts as |post|}}
      <tbody>
        <tr>
          <th scope="row">{{post.user.username}}</th>
          <td>{{post.createdAt}}</td>
          {{#each entries as |entry|}}
          <td>{{entry.exercise.exercise_name}}</td>
          <td>{{entry.set_count}}</td>
          <td>{{entry.rep_count}}</td>
          <td>{{entry.weight}}</td>
          <td>{{entry.effort}}</td>
          <td>{{entry.rest}}</td>
          {{/each}}
        </tr>
    {{/each}}
      </tbody>
    </table>


</section>